# Example environment variables for docker-compose
# Copy this file to .env (or pass --env-file) and replace values with secure secrets

# Keycloak admin
KEYCLOAK_ADMIN=admin
KEYCLOAK_ADMIN_PASSWORD=admin
KEYCLOAK_DB_PASSWORD=change_me

# Service DB passwords
PRODUIT_DB_PASSWORD=change_me
COMMANDE_DB_PASSWORD=change_me

# Frontend (for local dev)
REACT_APP_GATEWAY_URL=http://localhost:8080
REACT_APP_KEYCLOAK_URL=http://localhost:8180/auth
REACT_APP_KEYCLOAK_REALM=mini-projet
REACT_APP_KEYCLOAK_CLIENT=frontend
